body{
	margin:0;
	padding:0;
	background-color:#808080;
}
div{
	position:relative;
	float:left;
}

#div_container{
	width:80%;
	height:100%;
	border:0px solid blue;
	margin-left:10%;
	float:left;
	/*背景の色*/
	background-color:honeydew;
}

#div_fruit_list{
	width:100%;
}

#tbl_fruit{
	width:60%;
	/*テ`ブルの中の行の高さをO定*/
	line_height:28px;
	/*テ`ブルを下に移*/
	margin-top:120px;
	/*テ`ブルを右に移*/
	margin-left:20%
}

#tbl_fruit,#tbl_fruit tr,#tbl_fruit td{
	border:1px solid gray;
	/*セ`ルのボンドを合*/
	border-collapse:collapse;
	/*文字中央Bえ*/
	text-align:center;
	/*文字の式O定*/
	font-size:16px;
	font-weight:lighter;
	color:threeddarkshadow;
}
#tbl_fruit th{
	border:1px solid gray;
	/*セ`ルのボンドを合*/
	border-collapse:collapse;
	/*文字中央Bえ*/
	text-align:center;
	/*文字の式O定*/
	font-size:20px;
	font-weight:lighter;
	color:threeddarkshadow;
}

.w20{
	width:20%;
}
.delImg{
	width:24px;
	height:24px;
}
